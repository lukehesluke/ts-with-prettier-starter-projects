{
  "name": "@ts-with-prettier-starter-projects/browser-with-react",
  "version": "0.0.0",
  "description": "Fairly minimal browser TypeScript starter project",
  "scripts": {
    "ts-check": "tsc --noEmit",
    "test": "npm run lint && npm run prettier-format && npm run ts-check",
    "clean": "rm -rf dist",
    "build": "npm run clean && webpack",
    "build:watch": "npm run clean && webpack --watch",
    "serve": "cd dist && http-server",
    "lint": "eslint src --ext .ts --ext .tsx",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.{ts,tsx}' --write",
    "start:dev": "webpack serve --live-reload"
  },
  "author": "Luke Winship",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "^1.11.16",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "html-webpack-plugin": "^5.6.3",
    "http-server": "^14.1.1",
    "prettier": "^3.5.3",
    "swc-loader": "^0.2.6",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "engines": {
    "node": "22.14.0"
  }
}