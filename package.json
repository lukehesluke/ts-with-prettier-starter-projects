{
  "name": "@ts-with-prettier-starter-projects/branch-name",
  "version": "0.0.0",
  "description": "Fairly minimal TypeScript starter project",
  "scripts": {
    "ts-check": "tsc --noEmit",
    "test": "npm run lint && npm run prettier-format && npm run ts-check",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "author": "Luke Winship",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": "22.14.0"
  }
}